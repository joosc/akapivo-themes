name = Minorfront
description = A Bootstrap Sub-theme.
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[header]		= 'Header'
regions[content]	= 'Content'
regions[modal]  	= 'Modal'
regions[footer]   	= 'Footer'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/bootstrap-slider.css
stylesheets[all][] = css/style.css
stylesheets[all][] = css/bootstrap-switch.min.css
stylesheets[all][] = css/bebop.css


